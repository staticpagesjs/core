{
  "name": "@static-pages/core",
  "version": "0.0.0",
  "description": "General purpose static pages renderer.",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "browser": "lib/esm/index.js",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --outDir lib/esm",
    "build:cjs": "tsc --outDir lib/cjs --module commonjs",
    "watch:esm": "tsc --watch --outDir lib/esm",
    "watch:cjs": "tsc --watch --outDir lib/cjs --module commonjs",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staticpagesjs/core.git"
  },
  "keywords": [
    "static",
    "pages",
    "renderer",
    "mvc",
    "view",
    "controller",
    "transform",
    "template"
  ],
  "author": "László BULIK",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/staticpagesjs/core/issues"
  },
  "homepage": "https://github.com/staticpagesjs/core#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^12.20.20",
    "jest": "^27.0.6",
    "typescript": "^4.3.5"
  }
}
